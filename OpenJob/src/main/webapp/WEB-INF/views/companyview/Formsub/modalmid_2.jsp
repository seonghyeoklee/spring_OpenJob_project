<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<div class="modal-footer">
<div class="row" style="width:100%; margin:0px;">
<div class="col-xs-6" align="left" style="width:50%;">
	<button type="button" data-dismiss="modal" class="btn" id="closebtn" style="width:40%; height:30px; padding:0px; border-radius: 0px;">취소</button>
	<button type="button" onclick="showprev()" class="btn" id="prevbtn" style="width:40%; height:30px; padding:0px; border-radius: 0px; display:none;">이전</button>
</div>
<div class="col-xs-6" align="right" style="width:50%;">
	<button type="button" onclick="shownext()" class="btn" id="nextbtn" style="width:40%;height:30px; padding:0px; border-radius: 0px;">다음</button>
	<button type="button" onclick="submitfrm()" class="btn" id="submitbtn" style="width:40%;height:30px; padding:0px; border-radius: 0px; display:none;">제출</button>
</div>
</div>
</div>


